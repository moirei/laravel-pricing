{
    "name": "moirei/laravel-pricing",
    "description": "Easily manage complex pricing.",
    "homepage": "https://moirei.github.io/laravel-pricing",
    "license": "MIT",
    "authors": [
        {
            "name": "Augustus",
            "email": "augustusokoye@moirei.com"
        }
    ],
    "keywords":[
        "MOIREI",
        "ecommerce",
        "Laravel",
        "pricing",
        "volume",
        "graduated",
        "volume pricing",
        "graduated pricing"
    ],
    "scripts": {
        "test": "./vendor/bin/pest",
        "test-f": "vendor/bin/phpunit --filter"
    },
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/database": "^7|^8|^9",
        "laravel/helpers": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "MOIREI\\Pricing\\": "src",
            "MOIREI\\Pricing\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "orchestra/testbench": "^7.5"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
